#include<iostream>
#include<vector>
#include<algorithm>
 
using namespace std;
 
int main() 
{
    vector<int> numbers;
    int number;
    cout<<"Enter some numbers: ";
    while(cin>>number) 
    {
        numbers.push_back(number);}
        for(int i=0;i<numbers.size();i++)
            {
                sort(numbers.begin(),numbers.end());
                cout<<numbers[i]<<" ";
            }
    return 0;
}
